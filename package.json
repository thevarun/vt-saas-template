    "@electric-sql/pglite": "^0.2.12",
    "@hookform/resolvers": "^3.9.0",
    "@logtail/pino": "^0.5.2",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
