    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@sentry/nextjs": "^8.34.0",
    "@spotlightjs/spotlight": "^2.5.0",
    "@supabase/ssr": "0.8.0",
    "@supabase/supabase-js": "2.90.1",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@tanstack/react-table": "^8.20.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.562.0",
    "next": "^15.1.6",
    "next-intl": "^4.7.0",
    "next-themes": "^0.4.6",
    "pg": "^8.17.1",
    "pino": "^9.5.0",
    "pino-pretty": "^13.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.71.1",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.5",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.0.1",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@commitlint/cz-commitlint": "^20.3.1",
    "@eslint-react/eslint-plugin": "^2.7.2",
    "@faker-js/faker": "^9.0.3",
    "@next/bundle-analyzer": "^16.1.3",
    "@next/eslint-plugin-next": "^16.1.3",
    "@percy/cli": "1.30.1",
    "@percy/playwright": "^1.0.6",
    "@playwright/test": "^1.48.1",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/nextjs": "^10.1.11",
    "@storybook/react": "^10.1.11",
    "@storybook/test-runner": "^0.24.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.5.2",
    "@torka/claude-qol": "^0.1.2",
    "@torka/claude-workflows": "^0.6.3",
    "@types/node": "^25.0.7",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/expect": "^4.0.17",
    "autoprefixer": "^10.4.20",
    "checkly": "^4.9.0",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "cssnano": "^7.0.6",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.26.2",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.3.1",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-playwright": "^2.5.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-testing-library": "^7.15.4",
    "http-server": "^14.1.1",
    "husky": "^9.1.6",
    "jiti": "^1.21.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "rimraf": "^6.0.1",
    "semantic-release": "^25.0.2",
    "shadcn": "^3.7.0",
    "start-server-and-test": "^2.0.8",
    "storybook": "^10.1.11",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.1",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17",
    "vitest-fail-on-console": "^0.7.1"
  },
  "overrides": {
    "axios": "^1.13.2",
    "cookie": "^0.7.2",
    "esbuild": "^0.25.1",
    "glob": "^10.4.6",
    "webpack": "5.101.2"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
